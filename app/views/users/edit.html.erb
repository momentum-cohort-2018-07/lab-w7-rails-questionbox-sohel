<h1>Edit Account</h1>
<%= link_to "Back", user_path(current_user.id) %>
<div>
<%= form_with url: user_path(id:current_user.id), method: :put, scope: "user", local: true do |form| %>
  <div class="form-group">
  <%= form.label :username %>
  <%= form.text_field :username, class:"form-control" %>
  </div>
  <div class="form-group">
  <%= form.label :photo %>
  <%= form.file_field :photo, class:"form-control-file" %>
  </div>
  <div>
  <%= form.submit "Update", class:"btn btn-primary" %>
  </div>
<% end %>
</div>